{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true,
        "truffle/globals": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "warn"
    },
    "plugins": [
        "truffle"
    ]
}
